{
    "name"          : "solfegejs-cli",
    "description"   : "Command Line Interface of SolfegeJS",
    "version"       : "1.3.0",
    "author"        : "neolao <contact@neolao.com>",
    "repository"    : "https://github.com/neolao/solfege-bundle-cli.git",
    "license"       : "MIT",
    "main"          : "bundle-es5/index.js",
    "engines"       : 
    {
        "node": ">= 0.12"
    },
    "dependencies"  :
    {
        "solfegejs"     : "1.4.x",
        "colors"        : "1.0.x",
        "minimist"      : "1.1.x",
        "babel-runtime" : "5.6.15"
    },
    "devDependencies":
    {
        "babel"         : "5.6.14",
        "co"            : "*",
        "chai"          : "*",
        "mocha"         : "*",
        "istanbul-harmony": "0.3.x"
    },
    "scripts"       :
    {
        "test"          : "mocha --harmony --harmony-proxies --recursive --reporter spec --bail --async-only",
        "coverage"      : "node --harmony ./node_modules/istanbul-harmony/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
        "es6-watch"     : "node_modules/.bin/babel bundle-es6 --optional runtime --watch --out-dir bundle-es5"
    }
}
